= render partial: "shared/main-header"
- breadcrumb :likes
= render partial: "shared/bread_crumb"
%main.mypage-body
  = render partial: 'shared/mypage_side'
  .mypage-main
    %h2.header_title
      いいね！一覧
    .likes-lists
      - unless @products.count == 0
        %ul.show 
          - @products.each do |product|
            = render partial: 'shared/mypage_product_list', locals: { product: product }
      - else
        %ul.notice__none
          %li
            現在、いいね！した商品はありません
= render 'shared/sell-button'
= render 'shared/main-footer'